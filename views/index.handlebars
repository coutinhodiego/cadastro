<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Cadastros</title>
  </head>
  <body>
    <h1>{{title}}</h1>
    <a href="/novo">#Novo Aluno#</a><br><br>
    <table border="1">
      <tr>
        <th>Nome</th>
        <th>Idade</th>
        <th>Acoes</th>
      </tr>
      {{#each list}}
      <tr>
        <td>{{nome}}</td>
        <td>{{idade}}</td>
        <td><a href="/{{_id}}">Editar</a> | <a href="/del/{{_id}}">Excluir</a></td>
      </tr>
      {{/each}}
    </table>

  </body>
</html>
